<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="480.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controller.MenuController">
   <children>
      <Label fx:id="name" layoutX="37.0" layoutY="77.0" text="MineSweeperFX">
         <font>
            <Font name="Bauhaus 93" size="60.0" />
         </font>
      </Label>
      <Button fx:id="start" alignment="CENTER" layoutX="218.0" layoutY="135.0" mnemonicParsing="false" onAction="#startGame" style="-fx-alignment: center;" text="Start!" />
      <Button fx:id="leaderboard" layoutX="199.0" layoutY="166.0" mnemonicParsing="false" onAction="#showLeaderboard" text="Leaderboard" />
      <Button fx:id="close" layoutX="201.0" layoutY="561.0" mnemonicParsing="false" onAction="#close" text="Close game" />
   </children>
</AnchorPane>
